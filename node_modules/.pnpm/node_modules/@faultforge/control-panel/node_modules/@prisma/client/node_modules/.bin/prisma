#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/91914/Desktop/FaultForge/node_modules/.pnpm/prisma@7.4.2_@types+react@1_45e3fb1d4f70e4fb269860ffeecfa643/node_modules/prisma/node_modules:/mnt/c/Users/91914/Desktop/FaultForge/node_modules/.pnpm/prisma@7.4.2_@types+react@1_45e3fb1d4f70e4fb269860ffeecfa643/node_modules:/mnt/c/Users/91914/Desktop/FaultForge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/91914/Desktop/FaultForge/node_modules/.pnpm/prisma@7.4.2_@types+react@1_45e3fb1d4f70e4fb269860ffeecfa643/node_modules/prisma/node_modules:/mnt/c/Users/91914/Desktop/FaultForge/node_modules/.pnpm/prisma@7.4.2_@types+react@1_45e3fb1d4f70e4fb269860ffeecfa643/node_modules:/mnt/c/Users/91914/Desktop/FaultForge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../prisma/build/index.js" "$@"
else
  exec node  "$basedir/../../../../prisma/build/index.js" "$@"
fi
